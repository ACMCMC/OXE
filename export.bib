@article{RefWorks:doc:602fafe3c9e77c0001379be0,
	author={Emmanuel Francisco García Uribe and Larisa Ordóñez Molinar and Edgar Avilés Guillén},
	year={2016},
	month={Sep 20,},
	title={Motivaciones y obstáculos para el enprendedurismo: una perspectiva de género desde los jóvenes universitarios},
	journal={Retos},
	volume={6},
	number={11},
	pages={41},
	abstract={Cuando se habla de emprendimiento, no se puede pasar por alto la equidad de género. Se cree que la escasa implicación de las mujeres en  iniciativas emprendedoras se basa entre otros, en una mayor cantidad de obstáculos en comparación con los hombres, así como la diferencia en motivaciones entre ambos géneros. Es por eso que surge la pregunta, si las diferencias presentadas en estudios similares en distintos países del mundo también se dan en México, es decir si existe una diferencia significativa en las motivaciones y obstáculos para emprender un negocio entre mujeres y hombres universitarios.Este artículo hace un recorrido por conceptos que definen al emprendimiento, así como características personales del emprendedor que, sumados a políticas, programas e instituciones logran construir un entorno adecuado para la creación de nuevas empresas. Este trabajo, es una investigación cuantitativa de tipo correlacional, con un horizonte transversal, donde se tiene como objeto de estudio a estudiantes de la Universidad Autónoma de Ciudad Juárez. La evidencia sugiere que sí existe una diferencia significativa en las motivaciones para emprender un negocio entre hombres y mujeres que estudian en la Universidad Autónoma de Ciudad Juárez. Por el contrario, los obstáculos, no presentan una diferencia significativa entre los grupos},
	isbn={1390-6291},
	language={English},
	howpublished={\url{https://doaj.org/article/d4487cfaae7a4552b6284cad6d760f29}},
	doi={10.17163/ret.n11.2016.03}
}
